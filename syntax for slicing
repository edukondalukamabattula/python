a="Happybirthday"
b="123456"
c=" "
print(a.capitalize())
print(a.upper())
print(a.lower())
print(a.title())
print(a.swapcase())
print(a.split())
print(a.center(20,"m"))
print(a.count("p"))
print(a.replace("Happy","wish"))
print(a.join(b))
print(a.isupper())
print(a.islower())
print(a.isalpha())
print(a.isalnum())
print(b.isdigit())
print(c.isspace())
print(a.istitle())
print(a.startswith("H"))
print(a.endswith("y"))
print(a.find("p"))
print(len(a))
print(min(a))
print(max(a))


